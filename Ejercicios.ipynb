    "# Obtiene los n√∫meros primos menores a 100\n",
    "primos = []\n",
    "limite = 100\n",
    "for n in range(2, limite + 1):\n",
    "    for divisor in range(2, n):\n",
    "        if n % divisor == 0:\n",
    "            break\n",
    "    else:\n",
    "        primos.append(n)\n",
    "print(primos)"
